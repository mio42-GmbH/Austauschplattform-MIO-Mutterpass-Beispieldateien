      <fullUrl value="urn:uuid:c9a06c1e-b952-48ca-a764-89ef26c1b21a"/>
      <resource>
         <Composition xmlns="http://hl7.org/fhir">
            <id value="c9a06c1e-b952-48ca-a764-89ef26c1b21a"/>
